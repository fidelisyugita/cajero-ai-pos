	"scripts": {
		"start": "expo start",
		"reset-project": "node ./scripts/reset-project.js",
		"restart-adb": "adb kill-server && adb start-server",
		"android": "expo run:android --device",
		"android:clean": "cd android && ./gradlew clean && cd .. && expo run:android --device Medium_Tablet",
		"ios": "expo run:ios --device 'iPad Pro 11-inch (M5)'",
		"web": "expo start --web",
		"lint": "biome check .",
		"format": "biome format . --write",
		"build:apk": "cd android && ./gradlew assembleRelease",
		"build:apk:clean": "cd android && ./gradlew clean assembleRelease",
		"bundle:android": "cd android && ./gradlew bundleRelease",
		"maestro:sign-in": "maestro test --debug-output ./.maestro/output .maestro/tests/sign-in.yaml --env TEST_EMAIL=test@user.com --env TEST_PASSWORD=testPassword123"
